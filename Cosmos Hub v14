-- üõ°Ô∏è MOTOR DE EXECU√á√ÉO INFAL√çVEL (RETRY SYSTEM)
local function ForceRun(name, func)
    task.spawn(function()
        local success, err = pcall(func)
        if not success then 
            warn("Cosmos: Erro em " .. name .. ". For√ßando bypass...")
            task.wait(0.2)
            pcall(func)
        end
    end)
end

-- üóÇÔ∏è SISTEMA DE ABAS (GRADE DE COMANDO)
local Grid = Instance.new("UIGridLayout", Main)
Grid.CellSize = UDim2.new(0, 210, 0, 50)
Grid.Padding = UDim2.new(0, 10, 0, 10)
Grid.StartCorner = Enum.StartCorner.TopLeft

local function NewFunc(name, color, action)
    local btn = Instance.new("TextButton", Main)
    btn.Text = name
    btn.Font = Enum.Font.Code
    btn.TextSize = 17
    btn.TextColor3 = Color3.new(1,1,1)
    btn.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
    Instance.new("UIStroke", btn).Color = color
    btn.MouseButton1Click:Connect(function() ForceRun(name, action) end)
end

